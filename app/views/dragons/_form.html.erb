<%= form_for(dragon) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :power %>
  <%= f.text_field :power %>
  <%= f.label :level %>
  <%= f.text_field :level %>
  <%= f.label :age %>
  <%= f.text_field :age %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.file_field :photo %>
  <%= f.submit %>
<% end %>
